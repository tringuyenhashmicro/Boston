<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Sequences for Product Transfer -->
        <record id="seq_type_product_transfer_in" model="ir.sequence.type">
            <field name="name">Product Transfer IN</field>
            <field name="code">product.transfer.in</field>
        </record>

        <record id="seq_product_transfer_in" model="ir.sequence">
            <field name="name">Product Transfer IN</field>
            <field name="code">product.transfer.in</field>
            <field name="prefix">IN-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
	
		
		<record id="seq_type_product_transfer_out" model="ir.sequence.type">
            <field name="name">Product Transfer OUT</field>
            <field name="code">product.transfer.out</field>
        </record>

        <record id="seq_product_transfer_out" model="ir.sequence">
            <field name="name">Product Transfer OUT</field>
            <field name="code">product.transfer.out</field>
            <field name="prefix">OUT-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
		
		
    </data>
</openerp>
